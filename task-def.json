{
  "family": "flaskapp-task",
  "networkMode": "awsvpc",
  "containerDefinitions": [
    {
      "name": "flaskapp-img",
      "image": "flaskapp-img",
      "portMappings": [
        {
          "containerPort": 5000,
          "hostPort": 5000,
          "protocol": "tcp"
        }
      ]
    }
  ],
  "requiresCompatibilities": [
    "FARGATE"
  ],
  "cpu": "256",
  "memory": "512",
  "executionRoleArn": "arn:aws:iam::303981612052:role/ecsTaskExecutionRole"
}
